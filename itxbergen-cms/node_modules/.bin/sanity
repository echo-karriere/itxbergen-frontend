#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/sanity@3.80.1_@emotion+is-prop-valid@1.2.2_@types+node@22.13.10_@types+react@18.3.19_react-do_nps5md5xqsctzvihuzurjpbswm/node_modules/sanity/bin/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/sanity@3.80.1_@emotion+is-prop-valid@1.2.2_@types+node@22.13.10_@types+react@18.3.19_react-do_nps5md5xqsctzvihuzurjpbswm/node_modules/sanity/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/sanity@3.80.1_@emotion+is-prop-valid@1.2.2_@types+node@22.13.10_@types+react@18.3.19_react-do_nps5md5xqsctzvihuzurjpbswm/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/sanity@3.80.1_@emotion+is-prop-valid@1.2.2_@types+node@22.13.10_@types+react@18.3.19_react-do_nps5md5xqsctzvihuzurjpbswm/node_modules/sanity/bin/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/sanity@3.80.1_@emotion+is-prop-valid@1.2.2_@types+node@22.13.10_@types+react@18.3.19_react-do_nps5md5xqsctzvihuzurjpbswm/node_modules/sanity/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/sanity@3.80.1_@emotion+is-prop-valid@1.2.2_@types+node@22.13.10_@types+react@18.3.19_react-do_nps5md5xqsctzvihuzurjpbswm/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../sanity/bin/sanity" "$@"
fi

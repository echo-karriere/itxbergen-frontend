#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules/@sanity/runtime-cli/bin/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules/@sanity/runtime-cli/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules/@sanity/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules/@sanity/runtime-cli/bin/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules/@sanity/runtime-cli/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules/@sanity/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/@sanity+runtime-cli@1.6.0_@types+node@22.13.10/node_modules:/Users/isakhansen/ITx/itxbergen-frontend/itxbergen-cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run.js" "$@"
else
  exec node  "$basedir/../../bin/run.js" "$@"
fi
